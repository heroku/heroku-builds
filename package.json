{
  "name": "@heroku-cli/heroku-builds",
  "version": "0.0.30",
  "description": "Heroku CLI plugin to list and create builds for Heroku apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/heroku-build.git"
  },
  "engines": {
    "node": ">= 16.20.0"
  },
  "keywords": [
    "heroku-plugin"
  ],
  "author": "Heroku",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/heroku/heroku-build/issues"
  },
  "homepage": "https://github.com/heroku/heroku-build",
  "scripts": {
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "build": "rm -rf dist && tsc -b && oclif manifest && oclif readme && mv oclif.manifest.json ./dist/oclif.manifest.json && cp README.md ./dist/README.md && node scripts/prepack.mjs"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@heroku-cli/color": "2.0.1",
    "@heroku-cli/command": "^11.0.0",
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/core": "^2.8.11",
    "http-call": "^5.3.0",
    "ignore": "~ 5.3.1",
    "strftime": "^0.10.2",
    "tar": "^7.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.28",
    "@oclif/plugin-help": "^6.0.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "16.11.7",
    "@types/nock": "^10.0.3",
    "@types/strftime": "^0.9.8",
    "@types/tmp": "^0.2.6",
    "eslint": "8.x",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.26",
    "tsheredoc": "^1.0.1",
    "mocha-junit-reporter": "2.2.1",
    "mockdate": "3.0.5",
    "stdout-stderr": "^0.1.13",
    "tmp": "^0.2.3",
    "mocha": "10.x",
    "nock": "^13.5.4",
    "nyc": "^15.1.0",
    "oclif": "^4.10.5",
    "sinon": "^7.3.2",
    "ts-node": "10.x",
    "typescript": "5.x"
  },
  "files": [
    "/dist"
  ]
}
